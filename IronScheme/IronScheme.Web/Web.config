<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.web>
      <compilation debug="false"></compilation>
      <trust level="Full" />
      <httpHandlers>
        <add verb="*" type="IronScheme.Web.Handler, IronScheme" path="*.ss" />
      </httpHandlers>
      
    </system.web>
    <system.webServer>
      <validation validateIntegratedModeConfiguration="false"/>
        <handlers>
            <add name="IronScheme.Web" path="*.ss" verb="*" type="IronScheme.Web.Handler" resourceType="Unspecified" preCondition="integratedMode" />
        </handlers>
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By" />
                <add name="X-Powered-By" value="IronScheme.Web" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
